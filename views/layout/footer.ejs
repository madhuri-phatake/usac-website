<script>
    function makeActive(e) {
        localStorage.setItem("tab_name", $(e).attr("data-name"));
    }
    $(function () {
        let name = localStorage.getItem("tab_name");
        $("." + name).attr("class", name + " active open");
        $("." + name + "> a").attr("class", "waves-effect waves-cyan  active");
        $("#log_out_btn").on("click", function (e) {
            e.preventDefault();
            let user_type = "<%=user_type%>";
            $.post(this.href, function (data) {
                if (user_type == "admin" || user_type == "staff") {
                    location.href = "<%=baseURL%>" + "/login"
                } else {
                    location.href = "<%=baseURL%>" + "/vendor-login"
                }
            });
        });

        window.onscroll = function () {
            stick_top()
        };

        var header = document.getElementById("header");

        var sticky = header.offsetTop;

        function stick_top() {
            if (window.pageYOffset > sticky) {
                header.classList.add("sticky");
            } else {
                header.classList.remove("sticky");
            }
        }


    });

    function preventNumberInput(e) {
        var keyCode = (e.keyCode ? e.keyCode : e.which);
        if (keyCode > 47 && keyCode < 58 || keyCode > 95 && keyCode < 107) {
            e.preventDefault();
        }
    }

