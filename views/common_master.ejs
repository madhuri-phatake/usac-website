<script type="text/javascript">
    $(function () {
        let access_info = JSON.parse(("<%=user_access%>").split("&#34;").join('"'))[0];
        user.edit = access_info.web_master_access.indexOf("edit") > -1,
            user.view = access_info.web_master_access.indexOf("view") > -1,
            user.del = access_info.web_master_access.indexOf("delete") > -1,
            user.user_id = "<%=user_id%>";
        user.base_url = "<%=baseURL%>";
        user.init();
    });
    $(document).ready(function () {
        $('.datepicker').datepicker({
            dateFormat: 'dd-mm-yy',
            minDate: new Date()
        });
        $('.datepicker-1').datepicker({
