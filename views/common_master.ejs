<% include layout/header %>
<style>
    .a-101:hover {
        color: #000 !important;
    }

    .abc-101 {
        text-transform: capitalize !important;
    }
    .responsive-navbar-class::-webkit-scrollbar {
        width: 2px;
    }
    .responsive-navbar-class {
        overflow-x: scroll;
        overflow-y: hidden;
        display: flex;
        height: auto !important;
        flex-wrap: nowrap;
    }
</style>
<div id="main">
    <div class="row">
        <div class="content-wrapper-before transparent_bg"></div>
        <div class="col s12">
            <div class="container">
                <!-- users list start -->
                <section>
                    <div class="row">
                        <div class="col s12">
                            <nav class="nav-extended">
                                <div class="nav-content">
                                    <ul class="tabs tabs-transparent responsive-navbar-class">
                                        <li class="tab"><a class="active" href="#user_management">Users</a>
                                        </li>
                                     
                                    </ul>
                                </div>
                            </nav>
                        </div>
                        <!-- User management -->
                        <div id="user_management" class="col s12">
                            <section class="users-list-wrapper section">
                                <div class="users-list-filter">
                                    <div class="card-panel position-relative overflow-hidden">
                                        <form id="user_form">
                                            <fieldset class="access-controll">
                                                <div class="row">
                                                    <div class="input-field col l3 mb-0">
                                                        <input class="m-0" id="name" name="name"
                                                            onkeydown="preventNumberInput(event)"
                                                            onkeyup="preventNumberInput(event)" type="text">
                                                        <label for="name"> Name *</label>
                                                    </div>
                                                    <div class="input-field col l3 mb-0">
                                                        <input class="m-0" id="mobile_number" name="mobile_number"
                                                            type="number" maxlength="10">
                                                        <label for="mobile_number"> Mobile Number *</label>
                                                    </div>
                                                    <div class="input-field col l3 mb-0">
                                                        <input class="m-0" id="email" name="email" type="text">
                                                        <label for="email"> Email *</label>
                                                    </div>
                                                    <div class="input-field col l3 mb-0">
                                                        <input class="m-0" id="address" name="address" type="text">
                                                        <label for="address">Address *</label>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="input-field col l3 mb-0">
                                                        <select class="m-0 p-0 browser-default" id="user_type"
                                                            name="user_type">
                                                            <option value="">Select user type *</option>
                                                            <option value="admin">Admin</option>
                                                       
                                                        </select>
                                                    </div>
                                                    <div class="input-field col l3 mb-0">
                                                        <select class="m-0 p-0 browser-default" id="user_status"
                                                            name="user_status">
                                                            <option value="">Select user status *</option>
                                                            <option value="active">Active</option>
                                                            <option value="inactive">Inactive</option>
                                                        </select>
                                                    </div>
                                                    <!-- <div class="col l3 show-btn">
                                                        <button type="button" id="user_access"
                                                            class="btn btn-block flex-btn float-left waves-effect waves-light"
                                                            style="width: auto !important;color: green;
                                                            background-color: #fff;
                                                            font-weight: bold;">
                                                            <span id="user_access_btn" data-target="#user_access_modal">User Access</span>
                                                        </button>
                                                    </div> -->
                                                    <div class="col l3 show-btn">
                                                        <button type="submit" id="create_user"
                                                            class="btn btn-block flex-btn float-left indigo waves-effect waves-light"
                                                            style="width: auto !important">
                                                            <span id="create_user_btn">Add User</span>
                                                        </button>
                                                    </div>
                                                </div>
                                            </fieldset>
                                        </form>
                                    </div>
                                </div>
                                <input type="hidden" val="" id="user_id">
                                <div class="users-list-table">
                                    <div class="card">
                                        <div class="card-content">
                                            <!-- datatable start -->
                                            <div class="responsive-table">
                                                <table class="table" id="user_table">

                                                </table>
                                            </div>
                                            <!-- datatable ends -->
                                        </div>
                                    </div>
                                </div>
                            </section>
                        </div>
                    </div>
                </section>

            </div>
            <div class="content-overlay"></div>
        </div>
    </div>
</div>
